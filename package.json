{
  "version": "0.1.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaborVxxx/react-scrape.git"
  },
  "description": "React Typescript library with useful re-usable components and functions.",
  "keywords": [
    "react",
    "Typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format:C": "prettier --check .",
    "format:F": "prettier --write .",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "name": "react-scrape",
  "author": "GaborVxxx",
  "module": "dist/react-scrape.esm.js",
  "size-limit": [
    {
      "path": "dist/react-scrape.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-scrape.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@size-limit/preset-small-lib": "^8.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^3.9.10",
    "styled-components": "^5.3.6"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "react-icons": "^4.7.1"
  }
}
